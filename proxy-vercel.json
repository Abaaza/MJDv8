{
  "rewrites": [
    {
      "source": "/mjdpricing/(.*)",
      "destination": "https://your-mjd-app.vercel.app/$1"
    },
    {
      "source": "/mjdpricing",
      "destination": "https://your-mjd-app.vercel.app/"
    },
    {
      "source": "/",
      "destination": "https://your-main-site.vercel.app/"
    },
    {
      "source": "/(.*)",
      "destination": "https://your-main-site.vercel.app/$1"
    }
  ]
} 